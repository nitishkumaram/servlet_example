<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>servlet_example</display-name>
  <welcome-file-list>
    <welcome-file>MyIndex.jsp</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>context_username</param-name>
    <param-value>context_value</param-value>
  </context-param>
  <servlet>
    <servlet-name>register.do</servlet-name>
    <servlet-class>com.spaneos.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>register.do</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
</web-app>